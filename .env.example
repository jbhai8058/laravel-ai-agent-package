# AI Agents Package Configuration
# Set your preferred AI provider (openai or gemini)
AI_AGENT_DRIVER=openai

# General AI Agent Settings
AI_AGENT_MODEL=gpt-4
AI_AGENT_MEMORY_ENABLED=true
AI_AGENT_MEMORY_SIZE=10

# Database Schema Settings
AI_AGENT_LOAD_DB_SCHEMA=true
# AI_AGENT_EXCLUDED_TABLES=migrations,password_resets,personal_access_tokens,failed_jobs

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_DEFAULT_MODEL=gpt-4

# Google Cloud (Gemini) Configuration
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account.json
GEMINI_DEFAULT_MODEL=gemini-pro
