{
    "name": "muhammadjunaidrehmansiddiqui/laravel-ai-agents",
    "description": "A Laravel package for managing AI agents with support for OpenAI and Gemini",
    "type": "library",
    "version": "1.0.0",
    "license": "proprietary",
    "authors": [
        {
            "name": "Muhammad Junaid Rehman Siddiqui",
            "email": "junaidrehman0406@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jbhai8058/laravel-ai-agent-package"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "openai-php/client": "^0.6.0|^0.7.0|^0.8.0|^0.9.0|^0.10.0|^0.11.0|^0.12.0|^0.13.0|^0.14.0|^0.15.0|^0.16.0|^0.17.0",
        "google/cloud-ai-platform": "^0.7.0|^0.8.0|^0.9.0|^1.0.0|^1.1.0|^1.2.0|^1.3.0|^1.4.0|^1.5.0|^1.6.0|^1.7.0|^1.8.0|^1.9.0|^1.10.0|^1.11.0|^1.12.0|^1.13.0|^1.14.0|^1.15.0|^1.16.0|^1.17.0|^1.18.0|^1.19.0|^1.20.0|^1.21.0|^1.22.0|^1.23.0|^1.24.0|^1.25.0|^1.26.0|^1.27.0|^1.28.0|^1.29.0|^1.30.0|^1.31.0|^1.32.0|^1.33.0|^1.34.0|^1.35.0|^1.36.0|^1.37.0|^1.38.0|^1.39.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\Tests\\\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\AiAgentsServiceProvider"
            ],
            "aliases": {
                "AiAgent": "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\Facades\\\\AiAgent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
