{
    "name": "laravelai/smartagent",
    "description": "A Laravel package for managing AI agents with support for OpenAI and Gemini",
    "type": "library",
    "version": "1.3.2",
    "license": "proprietary",
    "authors": [
        {
            "name": "Muhammad Junaid Rehman Siddiqui",
            "email": "junaidrehman0406@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jbhai8058/laravel-ai-agent-package"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "ext-json": "*",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "openai-php/client": "^0.6.0|^0.7.0|^0.8.0|^0.9.0|^0.10.0|^0.11.0|^0.12.0|^0.13.0|^0.14.0|^0.15.0|^0.16.0|^0.17.0",
        "google/cloud-ai-platform": "^0.7.0|^0.8.0|^0.9.0|^1.0.0|^1.10.0|^1.20.0|^1.30.0|^1.39.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "suggest": {
        "ext-curl": "Required for HTTP requests to external services",
        "ext-mbstring": "Required for string manipulation functions"
    },
    "autoload": {
        "psr-4": {
            "LaravelAI\\SmartAgent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAI\\SmartAgent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAI\\SmartAgent\\AiAgentsServiceProvider"
            ],
            "aliases": {
                "AiAgent": "LaravelAI\\SmartAgent\\Facades\\AiAgent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
