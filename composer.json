{
    "name": "muhammadjunaidrehmansiddiqui/laravel-ai-agents",
    "description": "A Laravel package for managing AI agents with support for OpenAI and Gemini",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Junaid Rehman Siddiqui",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "openai-php/client": "^0.8.0",
        "google/cloud-ai-platform": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\Tests\\\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\AiAgentsServiceProvider"
            ],
            "aliases": {
                "AiAgent": "MuhammadJunaidRehmanSiddiqui\\\\AiAgents\\\\Facades\\\\AiAgent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
